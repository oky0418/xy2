module_folders = ['./modules/example1', './modules/example2', './modules/example3']

module_folders.each do |folder|
  begin
    FileUtils.mkdir_p(folder) unless Dir.exist?(folder)
    puts "get_user_id.rb        # 查看用户 ID '#{folder}' 已创建或已存在"
  rescue => e
    puts "创建文件夹 '#{folder}' 失败: #{e.message}"
  end
end